{
  "name": "strapi-plugin-bullmq-queue",
  "version": "0.1.1",
  "description": "A plugin to trigger BullMQ workflow from Strapi admin panel.",
  "strapi": {
    "name": "Publish Site",
    "icon": "plug",
    "description": "Publish changes and trigger worker via Redis."
  },
  "dependencies": {
    "bullmq": "^1.47.1"
  },
  "scripts": {
    "lint": "NODE_ENV=test npm-run-all --parallel lint:*",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:format": "prettier \"**/*.{md,html,yaml,yml,json}\" --check",
    "fix": "npm-run-all --sequential fix:*",
    "fix:js": "eslint \"**/*.{js,jsx}\" --fix",
    "fix:format": "prettier \"**/*.{md,html,yaml,yml,json}\" --write",
    "depcheck": "npx npm-check --update"
  },
  "author": "OblonDATA.IO Limited <contact@oblondata.io>",
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1"
  }
}
